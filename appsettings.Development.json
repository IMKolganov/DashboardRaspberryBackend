{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MicroserviceSettings": {
    "GetTemperatureAndHumidifyUrl": "http://localhost:5000",
    "GetSoilMoistureUrl": "http://localhost:5001"
  },
  "RabbitMqSettings": {
    "HostName": "localhost",
    "TimeoutSeconds": 5,
    "RequestQueues": [
      "backend.to.msmicrocontrollermanager.request"
    ],
    "ResponseQueues": [
      "msmicrocontrollermanager.to.backend.response"
    ]
  }
}
