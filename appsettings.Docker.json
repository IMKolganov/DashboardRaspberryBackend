{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MicroserviceSettings": {
    "GetTemperatureAndHumidifyUrl": "http://ms-get-temperature-and-humidify:5000",
    "GetSoilMoistureUrl": "http://ms-get-soil-moisture:5001"
  },
  "RabbitMqSettings": {
    "HostName": "rabbitmq",
    "RequestQueues": [
      "temperatureRequestQueue",
      "soilMoistureRequestQueue"
    ],
    "ResponseQueues": [
      "temperatureResponseQueue",
      "soilMoistureResponseQueue"
    ]
  }
}
